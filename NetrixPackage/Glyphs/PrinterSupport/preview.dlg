              <BUTTON id="butNextPage">
                <IMG id="next" src="res://Comzept.Genesis.NetRix.Resources.dll/PRINT/NextInactive" alt="Next Page (Alt+Right Arrow)"/>
              </BUTTON>
            </TD>
            <TD class="UIPane">
              <BUTTON id="butLastPage">
                <IMG id="end" src="res://Comzept.Genesis.NetRix.Resources.dll/PRINT/EndInactive" alt="Last Page (Alt+Ende)"/>
              </BUTTON>
            </TD>
            <TD class="UISeparator">
              <IMG width="0" height="0"/>
            </TD>
            <TD class="UIPane">
              <BUTTON id="butZoomOut">
                <IMG id="zoomOut" base="zoomOut" src="res://Comzept.Genesis.NetRix.Resources.dll/PRINT/Zoomout" alt="Zoom Out (Alt+Minus)"/>
              </BUTTON>
            </TD>
            <TD class="UIPane">
              <BUTTON id="butZoomIn">
                <IMG id="zoomIn" base="zoomIn" src="res://Comzept.Genesis.NetRix.Resources.dll/PRINT/Zoomin" alt="Zoom In (Alt+Plus)"/>
              </BUTTON>
            </TD>
            <TD class="UIPane">
              <SELECT id="selectZoom" accesskey="z">
                <OPTION VALUE="500" >500%</OPTION>
                <OPTION VALUE="200" >200%</OPTION>
                <OPTION VALUE="150" >150%</OPTION>
                <OPTION VALUE="100" >100%</OPTION>
                <OPTION VALUE="75" SELECTED="" >75%</OPTION>
                <OPTION VALUE="50" >50%</OPTION>
                <OPTION VALUE="25" >25%</OPTION>
                <OPTION VALUE="10" >10%</OPTION>
                <!-- ID's are for localization -->
                <OPTION VALUE="-1" id="idPageWidth">Whole Width</OPTION>
                <OPTION VALUE="-2" id="idWholePage">Whole Page</OPTION>
                <OPTION VALUE="-3" id="idTwoPages" >Two Pages </OPTION>
              </SELECT>
            </TD>
            <TD class="UISeparator" id="separatorFrameset" style="display:none">
              <IMG width="0" height="0"/>
              </TD>
            <TD class="UIPane" id="cellFrameset" style="display:none">
              <SELECT id="selectFrameset" accesskey="f">
                <OPTION VALUE="0" id="idLaidOut">As shown</OPTION>
                <OPTION VALUE="1" id="idSelected">Only current Frame</OPTION>
                <OPTION VALUE="2" id="idSeparate">Frame by Frame</OPTION>
              </SELECT>
            </TD>
            <TD class="UISeparator">
              <IMG width="0" height="0"/>
            </TD>
            <TD class="UIPane">
              <BUTTON id="butHelp" title="Druckvorschauhilfe (Alt+H)" accesskey="h">
                <U>H</U>elp
              </BUTTON>
            </TD>
            <TD class="UISeparator">
              <IMG width="0" height="0"/>
              </TD>
            <TD class="UIPane">
              <BUTTON id="butClose" title="Druckvorschau schließen (Alt+C)" accessKey="c">
                <U>C</U>lose
              </BUTTON>
            </TD>
          </TR>
        </TABLE>
      </DIV>
    </DIV>
    <DIV id="OverflowContainer" onclick="this.focus();" onfocus="butPrint.scrollIntoView();" tabindex="1" style="position:absolute; left:0; width:100%; overflow:auto; border:'thin threedhighlight inset'; background:threedshadow;">
      <DIV id="MasterContainer" tabindex="0" style="width:100%; position:absolute;">
